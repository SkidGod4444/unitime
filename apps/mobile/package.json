{
  "name": "@unitime/mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "APP_VARIANT=development expo start",
    "build:prod:i": "eas build --platform ios --profile production",
    "build:prod:a": "eas build --platform android --profile production",
    "build:preview:i": "eas build --platform ios --profile preview",
    "build:preview:a": "eas build --platform android --profile preview",
    "build:preview": "eas build --profile preview --channel preview",
    "build:prod": "eas build --profile production --channel prod",
    "build:dev:i": "APP_VARIANT=development eas build --platform ios --profile development",
    "build:dev:a": "APP_VARIANT=development eas build --platform android --profile development",
    "build:dev": "APP_VARIANT=development eas build --profile development",
    "android": "APP_VARIANT=development expo start --android",
    "ios": "APP_VARIANT=development expo start --ios",
    "web": "APP_VARIANT=development expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@unitime/auth": "workspace:*",
    "@unitime/db": "workspace:*",
    "@unitime/types": "workspace:*",
    "expo": "~54.0.30",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-localization": "^17.0.8",
    "expo-location": "^19.0.8",
    "expo-media-library": "^18.2.1",
    "expo-network": "^8.0.8",
    "expo-notifications": "^0.32.15",
    "expo-quick-actions": "^6.0.0",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.17",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@types/react": "~19.1.0",
    "babel-plugin-dotenv": "^0.1.1",
    "babel-preset-expo": "^54.0.9",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "~5.9.2"
  },
  "private": true
